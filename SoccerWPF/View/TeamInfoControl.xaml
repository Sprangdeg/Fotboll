<UserControl x:Class="SoccerWPF.View.TeamInfoControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SoccerWPF.View"
             mc:Ignorable="d" >
    <Grid>
        <TextBlock x:Name="textblock1" HorizontalAlignment="Left" Margin="20,54,0,0" TextWrapping="Wrap" Text="{Binding TeamInfoViewModel.MatchesHomeString}" 
                   VerticalAlignment="Top" Height="200" Width="300"/>
        <TextBlock x:Name="textblock2" HorizontalAlignment="Left" Margin="20,54,0,0" TextWrapping="Wrap" Text="{Binding TeamInfoViewModel.MatchesAwayString}" 
                   VerticalAlignment="Top" Height="200" Width="300"/>
    </Grid>
</UserControl>
